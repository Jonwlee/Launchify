{
  "manifest_version": 2,

  "name": "Launchify",
  "description": "This extension allows create and manage sets of pages that can be simulatinously launched", 
  "version": "1.0",
  "icons":{ 
    "16": "icon.png",
    "48": "icon.png",
    "128":"icon.png" },
  "author": "Jonathan Lee",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup/popup.html",
    "default_title": "Launchify"
  },
  "permissions": [
    "activeTab",

          "tabs",
          "storage",

    "https://ajax.googleapis.com/"
  ]
}
